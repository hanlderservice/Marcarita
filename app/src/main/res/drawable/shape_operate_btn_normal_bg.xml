<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 设置透明背景色 -->
    <solid android:color="@color/white" />

    <!-- 设置一个边框 -->
    <stroke
        android:width="1dp"
        android:color="@color/lightblue" />
    <padding
        android:bottom="3dp"
        android:left="8dp"
        android:right="8dp"
        android:top="3dp" />
    <!-- 设置四个圆角的半径 -->
    <corners android:radius="5dp" />


</shape>